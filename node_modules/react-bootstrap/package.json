    "classnames": "^2.3.1",
    "dom-helpers": "^5.2.1",
    "invariant": "^2.2.4",
    "prop-types": "^15.8.1",
    "prop-types-extra": "^1.1.0",
    "react-transition-group": "^4.4.2",
    "uncontrollable": "^7.2.1",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "@types/react": ">=16.14.8",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
